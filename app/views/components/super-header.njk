            <!--[if IE 8]>
            <img src="{{ params.assetsPath | default('/assets/images') }}/govuk-logotype-crown.png" class="govuk-header__logotype-crown-fallback-image" width="36" height="32" alt="">
            <![endif]-->
            <span class="govuk-header__logotype-text">
                GOV.UK
            </span>
          </span>
          {% if (params.productName) %}
            <span class="govuk-header__product-name">
              {{ params.productName }}
            </span>
          {% endif %}
        </a>
      </div>

      {% if params.serviceName or params.navigation %}
        <div class="govuk-super-header__content">
          {% if params.serviceName %}
            {% if params.serviceUrl %}
              <a href="{{ params.serviceUrl }}" class="govuk-header__link govuk-header__service-name">
                {{ params.serviceName }}
              </a>
              {% else%}
              <span class="govuk-header__service-name">
                {{ params.serviceName }}
              </span>
            {% endif %}
          {% endif %}

          <nav aria-label="{{ params.navigationLabel | default(menuButtonText) }}" class="govuk-super-header__navigation {{ params.navigationClasses if params.navigationClasses }}">
            <h2 id="super-navigation-menu-heading" class="govuk-visually-hidden">
                        Navigation menu
                    </h2>
            <div class="govuk-width-container gem-c-layout-super-navigation-header__button-width-container">
              <div class="gem-c-layout-super-navigation-header__button-container">
                <div id="menuButton" class="gem-c-layout-super-navigation-header__navigation-item">
                  <a id="menuLink" class="gem-c-layout-super-navigation-header__navigation-item-link" data-track-action="menuLink" data-track-category="headerClicked" data-track-label="/browse" data-track-dimension="Menu" data-track-dimension-index="29" href="https://gov.uk/browse">
                    <span class="gem-c-layout-super-navigation-header__navigation-item-link-inner">Menu</span>
                  </a>
                  <button id="jsMenuButton" hidden="hidden" aria-controls="super-navigation-menu-2191ea85" aria-expanded="false" aria-label="Show navigation menu" class="govuk-!-display-none gem-c-layout-super-navigation-header__navigation-top-toggle-button" data-text-for-hide="Hide navigation menu" data-text-for-show="Show navigation menu" data-toggle-desktop-group="top" data-toggle-mobile-group="top" data-tracking-key="menu" data-ga4-event="{&quot;event_name&quot;:&quot;select_content&quot;,&quot;type&quot;:&quot;header menu bar&quot;,&quot;text&quot;:&quot;Menu&quot;,&quot;index_section&quot;:1,&quot;index_section_count&quot;:2,&quot;section&quot;:&quot;Menu&quot;}" id="super-navigation-menu-2191ea85-toggle" type="button">
                    <span class="gem-c-layout-super-navigation-header__navigation-top-toggle-button-inner">Menu</span>
                  </button>
                </div>
                <div class="gem-c-layout-super-navigation-header__search-item">
                  <button id="super-search-menu-toggle" class="gem-c-layout-super-navigation-header__search-toggle-button govuk-!-display-none" aria-controls="super-search-menu" aria-expanded="false" aria-label="Show search menu" data-text-for-hide="Hide search menu" data-text-for-show="Show search menu" data-toggle-mobile-group="top" data-toggle-desktop-group="top" data-tracking-key="search" data-ga4-event="{&quot;event_name&quot;:&quot;select_content&quot;,&quot;type&quot;:&quot;header menu bar&quot;,&quot;text&quot;:&quot;Search&quot;,&quot;index_section&quot;:2,&quot;index_section_count&quot;:2,&quot;section&quot;:&quot;Search&quot;}" type="button">
                    <span class="govuk-visually-hidden">
                                Search GOV.UK
                    </span>
                    <svg class="gem-c-layout-super-navigation-header__search-toggle-button-link-icon" width="27" height="27" viewBox="0 0 27 27" fill="none" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" focusable="false">
                      <circle cx="12.0161" cy="11.0161" r="8.51613" stroke="currentColor" stroke-width="3"></circle>
                      <line x1="17.8668" y1="17.3587" x2="26.4475" y2="25.9393" stroke="currentColor" stroke-width="3"></line>
                    </svg>
                    <span aria-hidden="true" class="gem-c-layout-super-navigation-header__navigation-top-toggle-close-icon" focusable="false">
                      Ã—
                    </span>
                  </button>
                  <a id="searchButton" class="gem-c-layout-super-navigation-header__search-item-link" href="https://www.gov.uk/search">
                    <span class="govuk-visually-hidden">
                                Search GOV.UK
                    </span>
                    <svg class="gem-c-layout-super-navigation-header__search-item-link-icon" width="27" height="27" viewBox="0 0 27 27" fill="none" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" focusable="false">
                      <circle cx="12.0161" cy="11.0161" r="8.51613" stroke="currentColor" stroke-width="3"></circle>
                      <line x1="17.8668" y1="17.3587" x2="26.4475" y2="25.9393" stroke="currentColor" stroke-width="3"></line>
                    </svg>
                  </a>
                </div>
              </div>
            </div>
          </nav>
        </div>
      {% endif %}
    </div>
  </div>
</header>

